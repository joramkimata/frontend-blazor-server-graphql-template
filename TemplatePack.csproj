<?xml version="1.0" encoding="UTF-8"?>
<Project Sdk="Microsoft.NET.Sdk">
   <PropertyGroup>
      <PackageVersion>1.0</PackageVersion>
      <PackageId>IZWEB.BlazorServer.GraphQL.Templates</PackageId>
      <Title>IZWEB BlazorServer GraphQL Templates</Title>
      ​
      <Authors>Joram Kimata</Authors>
      ​
      <Description>Template for creating an ASP.NET web application with Auth0 authentication.</Description>
      ​
      <PackageTags>dotnet-new;templates;blazor-server;graphql</PackageTags>
      <TargetFramework>net7.0</TargetFramework>
      <IncludeContentInPack>true</IncludeContentInPack>
      <IncludeBuildOutput>false</IncludeBuildOutput>
      <ContentTargetFolders>content</ContentTargetFolders>
      <NoWarn>$(NoWarn);NU5128</NoWarn>
   </PropertyGroup>
   <ItemGroup>
      <Content Include="templates\**\*" Exclude="templates\**\bin\**;templates\**\obj\**" />
      <Compile Remove="**\*" />
   </ItemGroup>
</Project>